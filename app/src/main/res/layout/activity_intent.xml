<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".auth.IntentActivity">

    <TextView
        android:id="@+id/txt_profile"
        android:layout_width="107dp"
        android:layout_height="49dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/poppins"
        android:text="Profile "
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_get_address"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_border"
        android:fontFamily="@font/poppins"
        android:padding="10dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_address_title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_address_title"
        app:layout_constraintTop_toBottomOf="@+id/txt_address_title" />

    <TextView
        android:id="@+id/txt_fullname_title"
        android:layout_width="229dp"
        android:layout_height="23dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/poppins"
        android:text="Fullname"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_profile"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/txt_profile"
        app:layout_constraintTop_toBottomOf="@+id/txt_profile" />

    <TextView
        android:id="@+id/txt_username_title"
        android:layout_width="210dp"
        android:layout_height="19dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins"
        android:text="Username"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_get_fullname"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_get_fullname"
        app:layout_constraintTop_toBottomOf="@+id/txt_get_fullname" />

    <TextView
        android:id="@+id/txt_get_username"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/ic_border"
        android:fontFamily="@font/poppins"
        android:padding="10dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_username_title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_username_title"
        app:layout_constraintTop_toBottomOf="@+id/txt_username_title" />

    <TextView
        android:id="@+id/txt_email_title"
        android:layout_width="172dp"
        android:layout_height="21dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins"
        android:text="Email"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_get_username"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_get_username"
        app:layout_constraintTop_toBottomOf="@+id/txt_get_username" />

    <TextView
        android:id="@+id/txt_get_email"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_border"
        android:fontFamily="@font/poppins"
        android:padding="10dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_email_title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_email_title"
        app:layout_constraintTop_toBottomOf="@+id/txt_email_title" />

    <TextView
        android:id="@+id/txt_password_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins"
        android:text="Password"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_get_email"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_get_email"
        app:layout_constraintTop_toBottomOf="@+id/txt_get_email" />

    <TextView
        android:id="@+id/txt_get_password"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_border"
        android:fontFamily="@font/poppins"
        android:padding="10dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_password_title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_password_title"
        app:layout_constraintTop_toBottomOf="@+id/txt_password_title" />

    <TextView
        android:id="@+id/txt_sp_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins"
        android:text="Study Program"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_get_password"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_get_password"
        app:layout_constraintTop_toBottomOf="@+id/txt_get_password" />

    <TextView
        android:id="@+id/txt_get_sp"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_border"
        android:fontFamily="@font/poppins"
        android:padding="10dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_sp_title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_sp_title"
        app:layout_constraintTop_toBottomOf="@+id/txt_sp_title" />

    <TextView
        android:id="@+id/txt_date_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins"
        android:text="Date of Birth"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_get_sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_get_sp"
        app:layout_constraintTop_toBottomOf="@+id/txt_get_sp" />

    <TextView
        android:id="@+id/txt_get_date"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_border"
        android:padding="10dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_date_title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_date_title"
        app:layout_constraintTop_toBottomOf="@+id/txt_date_title" />

    <TextView
        android:id="@+id/txt_address_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins"
        android:text="Address"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_get_date"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_get_date"
        app:layout_constraintTop_toBottomOf="@+id/txt_get_date" />

    <TextView
        android:id="@+id/txt_get_fullname"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/ic_border"
        android:fontFamily="@font/poppins"
        android:padding="10dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txt_fullname_title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txt_fullname_title"
        app:layout_constraintTop_toBottomOf="@+id/txt_fullname_title" />

</androidx.constraintlayout.widget.ConstraintLayout>